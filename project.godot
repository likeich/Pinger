; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "HBoxContainer",
"class": "IPItem",
"language": "GDScript",
"path": "res://IPItem.gd"
} ]
_global_script_class_icons={
"IPItem": ""
}

[application]

config/name="Pinger"
run/main_scene="res://Main.tscn"
boot_splash/image="res://icon.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.305882, 0.305882, 0.305882, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://Oxygen-Icons.org-Oxygen-Categories-applications-internet.ico"

[display]

window/size/width=400
window/size/height=500
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="expand"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.305882, 0.305882, 0.305882, 1 )
environment/default_environment="res://default_env.tres"
